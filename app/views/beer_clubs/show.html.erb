<p id="notice"><%= notice %></p>

<p>
<h2><%= @beer_club.name %> <%= @beer_club.city %></h2>
<i>Founded <%= @beer_club.founded %> </i>
</p>
<p>
  Number of members <%= @beer_club.users.count %>.
</p>
<p>
  <h3>Members</h3>
    <ul>
  <% @beer_club.users.each do |user| %>
      <li><%= link_to user.username, user %></li>
  <% end %>
      </ul>
</p>


