<h1>Sign in</h1>
<p id="notice"><%= notice %></p>
<%= form_tag sessions_path do %>
    <%= label_tag 'username' %>
    <%= text_field_tag :username, params[:username] %> <br />
    <%= label_tag 'password' %>
    <%= password_field_tag :password, params[:password] %>
    <%= submit_tag "Log in" %>
<% end %>